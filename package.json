{
  "name": "easy-note-shx",
  "version": "1.0.0",
  "description": "清记",
  "main": "dist/main.js",
  "module": "dist/main.js",
  "types": "dist/esm/main/index.d.ts",
  "files": [
    "./dist"
  ],
  "scripts": {
    "startWebpack": "concurrently \"npm:webpackDev*\"",
    "startElectron": "electron ./dist/main.js --pre",
    "rebuild": "electron-rebuild -f -w better-sqlite3",
    "webpackDevReact": "webpack --config=./webpack/webpack.config.react.js --env=mode=development",
    "webpackDevElectron": "webpack --config=./webpack/webpack.config.electron.js --env=mode=development",
    "prepare": "husky install",
    "lint": "echo \"Now will run eslint, please waiting moment... \n\" && eslint --ext .ts --ext .tsx src/",
    "ts": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "buildWebpack": "concurrently \"npm:webpackBuild*\"",
    "webpackBuildReact": "webpack --config=./webpack/webpack.config.react.js --env=mode=production",
    "webpackBuildElectron": "webpack --config=./webpack/webpack.config.electron.js --env=mode=production",
    "packageMac": "electron-builder --mac --config ./electron-builder.json",
    "packageWin": "electron-builder --win --config ./electron-builder.json",
    "build": "yarn buildWebpack && packageMac && packageWin",
    "buildMac": "yarn buildWebpack && packageMac"
  },
  "author": "shx",
  "license": "ISC",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "electron": "^35.1.5",
    "electron-builder": "^26.0.12",
    "eslint": "^8.35.0",
    "eslint-config-alloy": "^4.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.3",
    "prettier": "^2.3.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "typescript": "^4.3.5",
    "webpack": "5.76.0",
    "webpack-cli": "5.1.1",
    "webpack-dev-server": "^4.0.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "antd": "^5.24.9",
    "axios": "^1.8.4",
    "better-sqlite3": "^11.9.1",
    "electron-store": "^10.0.1",
    "eventemitter3": "^5.0.1",
    "js-md5": "^0.8.3",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.1.5",
    "query-string": "^9.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "slate": "^0.112.0",
    "slate-dom": "^0.111.0",
    "slate-history": "^0.113.1",
    "slate-react": "^0.112.1",
    "styled-components": "^6.1.17"
  }
}
